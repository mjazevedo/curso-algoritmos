algoritmo "quadrado_diferenca"
// Função :
// Autor :
// Data : 21/08/2021
// Seção de Declarações 
var
   a, b, resultado : real
inicio
// Seção de Comandos 
   escreval("Digite dois valores: ")
   leia(a)
   leia(b)
   
   resultado <- Exp(a,2) - 2 * a * b + Exp(b,2)
   
   escreva("Resultado: ", resultado)
   
fimalgoritmo
