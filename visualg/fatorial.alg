algoritmo "semnome"
// Função :
// Autor :
// Data : 06/08/2021
// Seção de Declarações 
var
   n, i, fatorial : inteiro
inicio
// Seção de Comandos 
   escreva("Digite o valor de N: ")
   leia(n)

   fatorial <- 1

   para i de 1 ate n faca
        fatorial <- fatorial * i
   fimpara
   
   escreva("FATORIAL = ", fatorial)
   
fimalgoritmo
